= form_for @profile_role do |f|
  - if @profile_role.errors.any?
    #error_explanation
      %h2= "#{pluralize(@profile_role.errors.count, "error")} prohibited this profile_role from being saved:"
      %ul
        - @profile_role.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :profle_id
    = f.number_field :profle_id
  .field
    = f.label :role_id
    = f.number_field :role_id
  .actions
    = f.submit 'Save'
