      - @active_season.divisions.each do |division|
        - division.teams.each do |team|
          - team.rosters.each do |player|
            %Row
              %Cell
                %Data{"ss:Type" => "String"}=player.profile.first_name
              %Cell
                %Data{"ss:Type" => "String"}=player.profile.last_name
              %Cell
                %Data{"ss:Type" => "String"}=player.profile.email
              %Cell
                %Data{"ss:Type" => "String"}=player.profile.dob
              %Cell
                %Data{"ss:Type" => "Number"}
              %Cell
                %Data{"ss:Type" => "String"}=division.description
              %Cell
                %Data{"ss:Type" => "String"}=team.name
              %Cell
                %Data{"ss:Type" => "String"}
              -if player.profile.rating
                %Cell
                  %Data{"ss:Type" => "Number"}=player.profile.rating.total
                %Cell
                  %Data{"ss:Type" => "Number"}=player.profile.rating.rating_1
                %Cell
                  %Data{"ss:Type" => "Number"}=player.profile.rating.rating_2
              - else
                %Cell
                  %Data{"ss:Type" => "String"} No Player Rating