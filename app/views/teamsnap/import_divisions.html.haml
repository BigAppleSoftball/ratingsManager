- content_for :panel_head do
  %h1 Importing Divisions


%ul.list-group
  -@divisions.each do |division|
    %li.list-group-item
      = division[:name]
      .teamsnap_id= division[:teamsnap_id]
      .division_type=division[:type]
      %ul.list-group
        - division[:teams].each do |team|
          %li.list-group-item
            = team[:name]
            = team[:teamsnap_id]
            %ul.list-group
              - team[:roster].each do |player|
                %li.list-group-item
                  = player