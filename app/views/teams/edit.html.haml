- content_for :panel_head do
  %h1 
    Editing 
    =@team.name
    - if @team.teamsnap_id
      %span.small-text 
        (Imported from teamsnap:
        = @team.teamsnap_id
        )
  %h4= link_to "#{@team.division.season.description} #{@team.division.description}", @team.division
  = render partial: "shared/actions/edit_actions", locals: { allPath: teams_path, fieldName: 'Team', showPath: @team}

= render 'form'

